		"@types/resize-observer-browser": "^0.1.4",
		"cross-env": "^7.0.2",
		"eslint-plugin-svelte3": "^2.7.3",
		"marked": "^2.0.0",
		"npm-run-all": "^4.1.5",
		"remarkable": "^2.0.1",
